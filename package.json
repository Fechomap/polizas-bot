{
  "name": "polizas-bot",
  "version": "1.0.0",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "start:dev": "node src/bot.js",
    "start:ts": "ts-node src/bot.ts",
    "dev": "nodemon --exec ts-node src/bot.ts",
    "dev:js": "nodemon src/bot.js",
    "build": "tsc",
    "build:prod": "tsc --project tsconfig.prod.json",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "type-check:prod": "tsc --project tsconfig.prod.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ scripts/",
    "lint:fix": "eslint src/ scripts/ --fix",
    "lint:ts": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"types/**/*.{ts,d.ts}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"types/**/*.{ts,d.ts}\"",
    "reinstall": "rimraf node_modules package-lock.json && npm cache clean --force && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@bull-board/express": "^6.14.2",
    "@telegraf/session": "^2.0.0-beta.7",
    "@types/bull": "^3.15.9",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.8.4",
    "bull": "^4.16.5",
    "bull-board": "^1.7.2",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "ioredis": "^5.8.2",
    "moment-timezone": "^0.5.48",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.3",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pdfkit": "^0.17.1",
    "rimraf": "^5.0.10",
    "sharp": "^0.34.5",
    "telegraf": "^4.16.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.14",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "@types/pdfkit": "^0.17.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "mongodb-memory-server": "^10.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
